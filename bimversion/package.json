{
  "name": "vue-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@xkeshi/image-compressor": "^0.5.3",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "cropperjs": "^1.5.4",
    "element-ui": "^2.10.1",
    "good-storage": "^1.1.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.0.7",
    "vue-simple-uploader": "^0.7.1",
    "vue-upload-component": "^2.8.20",
    "vuetify": "^1.5.16",
    "vuex": "^3.0.1",
    "vx-easyui": "^1.1.25"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "indent": 0,
      "semi": [
        "error",
        "always"
      ],
      "no-unneeded-ternary": 0,
      "no-unused-expressions": 0,
      "space-before-function-paren": [
        "error",
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
